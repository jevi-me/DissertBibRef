@article{trussler_2014,
	title = {Consumer demand for cynical and negative news frames},
	author = {Trussler, Marc and Soroka, Stuart},
	pages = {360-379},
	url = {http://journals.sagepub.com/doi/10.1177/1940161214524832},
	year = {2014},
	month = {jul},
	urldate = {2021-03-26},
	journal = {The International Journal of Press/Politics},
	volume = {19},
	number = {3},
	issn = {1940-1612},
	doi = {10.1177/1940161214524832},
	sciwheel-projects = {{OCADU} {MFA} Thesis},
	abstract = {Commentators regularly lament the proliferation of both negative and/or strategic (\textquotedbllefthorse race\textquotedblright) coverage in political news content. The most frequent account for this trend focuses on news norms and/or the priorities of news journalists. Here, we build on recent work arguing for the importance of demand-side, rather than supply-side, explanations of news content. In short, news may be negative and/or strategy-focused because that is the kind of news that people are interested in. We use a lab study to capture participants\textquoteright news-selection biases, alongside a survey capturing their stated news preferences. Politically interested participants are more likely to select negative stories. Interest is associated with a greater preference for strategic frames as well. And results suggest that behavioral results do not conform to attitudinal ones. That is, regardless of what participants say, they exhibit a preference for negative news content.}
}
