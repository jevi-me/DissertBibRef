@article{chapman_2012,
	title = {Research-Creation: Intervention, Analysis and "Family Resemblances"},
	author = {Chapman, Owen B. and Sawchuk, Kim},
	url = {http://www.cjc-online.ca/index.php/journal/article/view/2489},
	year = {2012},
	month = {apr},
	day = {13},
	urldate = {2020-07-19},
	journal = {Canadian Journal of Communication},
	volume = {37},
	number = {1},
	issn = {1499-6642},
	doi = {10.22230/cjc.2012v37n1a2489},
	sciwheel-projects = {{OCADU} {MFA} Thesis and {OCADU} {MFA} Thesis - Proposed Reading List},
	abstract = {{ABSTRACT} {\textquotedblleftResearch}-creation\textquotedblright is an emergent category within the social sciences and humanities that speaks to contemporary media experiences and modes of knowing. Research-creation projects typically integrate a creative process, experimental aesthetic component, or an artistic work as an integral part of a study. The focus of this article is how this practice contributes to the research agenda of the digital humanities and social sciences. We discuss how the term has been articulated in academic policy discourses and examine some prominent academic analyses that describe the practice of research-creation. We then unravel, enumerate, and expand upon the concept of research-creation from the purview of Ludwig Wittgenstein\textquoterights notion of \textquotedblleftfamily resemblances\textquotedblright before moving to a discussion of four modes of research-creation: \textquotedblleftresearch-for-creation,\textquotedblright \textquotedblleftresearch-from-creation,\textquotedblright \textquotedblleftcreative presentations of research,\textquotedblright and finally \textquotedblleftcreation-as-research.{\textquotedblrightR}É{SUM}É La « recherche-création » est un domaine naissant en sciences humaines où l\textquoterighton s\textquoterightintéresse aux expériences médiatiques et aux manières de savoir contemporaines. Les projets de recherche-création comportent typiquement un processus créatif, une composante esthétique expérimentale ou une oeuvre artistique. Notre article porte sur la manière dont cette pratique contribue à l\textquoterightagenda de recherche en digital humanities (« humanités numériques ») et en sciences humaines. Nous relevons comment les discours académiques sur les politiques ont traité de la recherche-création et examinons des analyses académiques importantes qui dressent le portrait de celle-ci. Ensuite, nous évaluons et élargissons ce concept en ayant recours à l\textquoterightidée des « ressemblances de famille » telle que développée par Ludwig Wittgenstein, puis pour terminer nous commentons quatre modes de recherche-création : « la recherche pour la création », « la recherche à partir de la création », « les présentations de recherche créatives » et « la création sous forme de recherche ».}
}
